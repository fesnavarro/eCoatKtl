	END_REGION
	REGION red
	    #action^.red[0].i += 1;
	    "=pre-79H1:1" := #action^.red[#action^.red[0].i].signal;//ALARME VISUAL FALHA/DEFEITO
	    #action^.red[0].i += 1;
	    "=pre-85KA6:A1" := #action^.red[#action^.red[0].i].signal;//ALARME VISUAL FALHA/DEFEITO
