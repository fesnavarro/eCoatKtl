	    #action^.transferPumpTank[0].i += 3;
	    "=ecoat-47KA1:A1" := #action^.transferPumpTank[#action^.transferPumpTank[0].i].signal;//LIGA BOMBA TANQUE 14 RESERVA TINTA KTL
	    #action^.transferPumpTank[0].i += 5;
